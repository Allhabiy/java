<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<VBox maxWidth="-Infinity" minWidth="-Infinity" prefHeight="200.0" prefWidth="230.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="GUIControl.GUISignup">
   <children>
      <Label text="Sign Up">
         <font>
            <Font size="24.0" />
         </font>
         <padding>
            <Insets bottom="30.0" />
         </padding>
      </Label>
      <Label text="First Name" />
      <TextField fx:id="firstName" onAction="#handleButtonAction">
         <VBox.margin>
            <Insets bottom="15.0" />
         </VBox.margin>
      </TextField>
      <Label layoutX="10.0" layoutY="10.0" text="Last Name" />
      <TextField fx:id="lastName" onAction="#handleButtonAction">
         <VBox.margin>
            <Insets bottom="15.0" />
         </VBox.margin>
      </TextField>
      <Label layoutX="10.0" layoutY="27.0" text="Email" />
      <TextField fx:id="email" onAction="#handleButtonAction">
         <VBox.margin>
            <Insets bottom="15.0" />
         </VBox.margin>
      </TextField>
      <Label text="Phone Number" />
      <TextField fx:id="phone" layoutX="10.0" layoutY="69.0" onAction="#handleButtonAction">
         <VBox.margin>
            <Insets bottom="15.0" />
         </VBox.margin>
      </TextField>
      <Label text="Error" textFill="RED">
         <VBox.margin>
            <Insets bottom="15.0" />
         </VBox.margin>
      </Label>
      <HBox prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button mnemonicParsing="false" text="Back" />
            <Region HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </Region>
            <Button defaultButton="true" mnemonicParsing="false" text="Sign Up" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="30.0" left="30.0" right="30.0" top="30.0" />
   </padding>
</VBox>
